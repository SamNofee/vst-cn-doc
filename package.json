{
  "name": "vst-cn-doc",
  "version": "1.0.0",
  "description": "The documentation of VST/VST3 of Chinese",
  "exports": "./md2docs.ts",
  "author": "SamNofee",
  "type": "module",
  "license": "GPL3.0",
  "scripts": {
    "md2docs": "node --loader ts-node/esm --es-module-specifier-resolution=node md2docs",
    "test": "npx mocha"
  },
  "engines": {
    "node": ">=16.17"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "marked": "^4.2.5"
  },
  "devDependencies": {
    "@types/marked": "^4.0.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.2",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": "*.test.ts",
    "node-option": [
      "experimental-specifier-resolution=node",
      "loader=ts-node/esm"
    ]
  }
}
